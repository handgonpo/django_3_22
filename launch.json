{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Django Run",
        "type": "python",
        "request": "launch",
        "program": "${workspaceFolder}/manage.py",
        "args": [
          "runserver"
        ],
        "django": true,
        "env": {
          "DJANGO_SETTINGS_MODULE": "config.settings.local"
        }
      }
    ]
  }
  